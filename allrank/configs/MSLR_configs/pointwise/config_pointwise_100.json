    "metrics": [
        "ndcg_10",
        "ndcg_50",
        "ndcg_100",
        "mrr_10",
        "mrr_50",
        "mrr_100"
    ],
    "loss": {
        "name": "pointwise_rmse",
        "args": {
            "no_of_levels": 5
        }
