{
    "model": {
        "fc_model": {
            "sizes": [
                128
            ],
            "input_norm": false,
            "activation": null,
            "dropout": 0.3
        },
        "transformer": {
            "N": 4,
            "d_ff": 64,
            "h": 4,
            "positional_encoding": null,
            "dropout": 0.3
        },
        "post_model": {
            "output_activation": "Sigmoid",
